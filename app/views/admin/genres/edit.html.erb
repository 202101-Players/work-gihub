<div class="container px-5 px-sm-0">
    <h1>ジャンル編集</h1>
    <div class="row">
    <%= form_with model:@genre, url:admin_genre_path, local:true do |f| %>
      <div class="form-group">
          <%= f.label :name %>
          <%= f.text_field :name, class: 'form-control book_title' %>
      </div>
      <div class="form-group">
        <%= f.submit '変更を保存', class: 'btn btn-success' %>
      </div>
      <% end %>
   </div>
 </div>