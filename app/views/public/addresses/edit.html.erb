<h1>配送先編集</h1>
<%= form_with model:@address, url:public_address_path, local:true do |f| %>
 <div>
  <%= f.label :"郵便番号（ハイフンなし）" %>
  <%= f.text_field :postal_code %>
 </div>
 <div>
  <%= f.label :"住所" %>
  <%= f.text_field :address %>
 </div>
 <div>
  <%= f.label :"宛名" %>
  <%= f.text_field :name %>
 </div>
  <%= f.submit '変更する' %>
<% end %>